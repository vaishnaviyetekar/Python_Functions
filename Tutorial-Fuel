num=int(input("Enter number the cost price:"))


if num<=50000:
    print("You need to pay 5% tax")

elif num>50000 and num<=100000:
    print("You need to pay 10% tax")

elif num<=100000:
    print("You need to pay 15% tax")
    
else:
    print("You need to pay more than 15% tax")
    
